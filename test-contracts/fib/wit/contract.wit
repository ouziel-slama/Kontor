package root:component;

world root {
  include kontor:built-in/built-in;
  use kontor:built-in/foreign.{contract-address};
  use kontor:built-in/context.{fall-context, view-context, proc-context, signer};
  use kontor:built-in/error.{error};

  export init: async func(ctx: borrow<proc-context>);

  export fib: async func(ctx: borrow<proc-context>, arith-address: contract-address, n: u64) -> u64;

  export fib-of-sub: async func(ctx: borrow<proc-context>, arith-address: contract-address, x: string, y: string) -> result<u64, error>;

  export cached-values: async func(ctx: borrow<view-context>) -> list<u64>;
}
