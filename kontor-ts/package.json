    "test:browser": "vitest --browser"
  },
  "devDependencies": {
    "@bytecodealliance/jco": "^1.15.3",
    "@vitest/browser-playwright": "^4.0.15",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12"
  }
