{
  "name": "@kontor/kontor-ts",
  "version": "0.2.0",
  "author": "Unspendable Labs <dev@unspendablelabs.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KontorProtocol/Kontor.git",
    "directory": "kontor-ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
