package component:fib;

world fib {
    resource monoid {
        constructor(address: u64);
        mzero: func() -> u64;
        mappend: func(x: u64, y: u64) -> u64;
    }

    export fib: func(n: u64) -> u64;

    export api-twice: func(address: u64, n: u64) -> u64;
    export twice: func(m: monoid, n: u64) -> u64;
}
